  "license": "MIT",
  "dependencies": {
    "chart.js": "^2.7.0",
    "d3": "^4.11.0",
    "force-array": "^3.1.0",
    "fs": "^0.0.1-security",
    "joyent-ui-toolkit": "^2.1.0",
