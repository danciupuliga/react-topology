    "bundlesize": "^0.15.3",
    "css-loader": "^0.28.7",
    "enzyme": "^3.0.0",
    "eslint": "^4.7.2",
    "eslint-config-joyent-portal": "^3.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-react-app": "^2.0.1",
