    "styled-is": "^1.1.0"
  },
  "scripts": {
    "start": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "build": "NODE_ENV=production babel src --out-file lib/index.js --ignore spec.js,test.js",
    "lint": "eslint src",
    "jest": "jest --env=jsdom",
